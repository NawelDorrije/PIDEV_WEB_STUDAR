{% extends 'base.html.twig' %}

{% block title %}ImageLogement index{% endblock %}

{% block body %}
    <h1>ImageLogement index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Url</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for image_logement in image_logements %}
            <tr>
                <td>{{ image_logement.id }}</td>
                <td>{{ image_logement.url }}</td>
                <td>
                    <a href="{{ path('app_image_logement_show', {'id': image_logement.id}) }}">show</a>
                    <a href="{{ path('app_image_logement_edit', {'id': image_logement.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="4">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_image_logement_new') }}">Create new</a>
{% endblock %}
